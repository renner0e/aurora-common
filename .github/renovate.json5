{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>ublue-os/renovate-config:org-inherited-config"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^Containerfile$/"],
      "matchStrings": [
        "https:\\/\\/github\\.com\\/renner0e\\/artwork\\/releases\\/download\\/(?<currentValue>.*?)\\/aurora-wallpapers\\.tar\\.zstd"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "renner0e/artwork",
      "extractVersionTemplate": "aurora-v{{version}}"
    }
  ]
}

